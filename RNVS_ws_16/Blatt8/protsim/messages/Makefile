# Name of target to be created (-o option)
TARGET = messages

# User interface (uncomment one) (-u option)
# USERIF_LIBS=$(CMDENV_LIBS)
USERIF_LIBS=$(TKENV_LIBS)

# .ned or .h include paths with -I
INCLUDE_PATH= -I../common

# misc additional object and library files to link
EXTRA_OBJS=

# object files from other directories to link with (wildcard needed to prevent "no such file *.o" errors)
EXT_DIR_OBJS=

# time stamps of other directories (used as dependency)
EXT_DIR_TSTAMPS= ../common/.tstamp

# Additional libraries (-L option -l option)
LIBS=

#------------------------------------------------------------------------------
# Import generic settings from ../omnetppconfig
include ../omnetppconfig

MSGCOPTS= $(INCLUDE_PATH)

#------------------------------------------------------------------------------

# subdirectories to recurse into
SUBDIRS= 

# object files in this directory
OBJS=  ARQMessage_m.o LocalLinkChangeNotif_m.o LocalPacketNotif_m.o NetworkPacket_m.o PingMessage_m.o 

# header files generated (from msg files)
GENERATEDHEADERS= ARQMessage_m.h LocalLinkChangeNotif_m.h LocalPacketNotif_m.h NetworkPacket_m.h PingMessage_m.h

#------------------------------------------------------------------------------
# contents of file makefrag:
all: $(TARGET)
DistanceVectorMessage_m.cc: NetworkPacket_m.cc
LinkStateAdvertisement_m.cc: NetworkPacket_m.cc
LocalLinkChangeNotif_m.cc: NetworkPacket_m.cc
LocalPacketNotif_m.cc: NetworkPacket_m.cc
PingMessage_m.cc: NetworkPacket_m.cc
ARQMessage_m.cc: NetworkPacket_m.cc

#------------------------------------------------------------------------------

$(TARGET): $(OBJS) Makefile .tstamp
	@# do nothing

.tstamp: $(OBJS)
	echo>.tstamp

$(OBJS) : $(GENERATEDHEADERS)


purify: $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_TSTAMPS) Makefile
	purify $(CXX) $(LDFLAGS) $(OBJS) $(EXTRA_OBJS) $(EXT_DIR_OBJS) $(LIBS) -L$(OMNETPP_LIB_DIR) $(KERNEL_LIBS) $(USERIF_LIBS) $(SYS_LIBS_PURE) -o $(TARGET).pure

$(EXT_DIR_TSTAMPS):
	@echo
	@echo Error: $@ does not exist.
	@echo This means that at least the above dependency directory has not been built.
	@echo Maybe you need to do a top-level make?
	@echo
	@exit 1

.PHONY: subdirs $(SUBDIRS)

subdirs: $(SUBDIRS)

ARQMessage_m.o: ARQMessage_m.cc
	$(CXX) -c $(NEDCOPTS) ARQMessage_m.cc

ARQMessage_m.cc ARQMessage_m.h: ARQMessage.msg
	$(MSGC) $(MSGCOPTS) ARQMessage.msg

LocalLinkChangeNotif_m.o: LocalLinkChangeNotif_m.cc
	$(CXX) -c $(NEDCOPTS) LocalLinkChangeNotif_m.cc

LocalLinkChangeNotif_m.cc LocalLinkChangeNotif_m.h: LocalLinkChangeNotif.msg
	$(MSGC) $(MSGCOPTS) LocalLinkChangeNotif.msg

LocalPacketNotif_m.o: LocalPacketNotif_m.cc
	$(CXX) -c $(NEDCOPTS) LocalPacketNotif_m.cc

LocalPacketNotif_m.cc LocalPacketNotif_m.h: LocalPacketNotif.msg
	$(MSGC) $(MSGCOPTS) LocalPacketNotif.msg

NetworkPacket_m.o: NetworkPacket_m.cc
	$(CXX) -c $(NEDCOPTS) NetworkPacket_m.cc

NetworkPacket_m.cc NetworkPacket_m.h: NetworkPacket.msg
	$(MSGC) $(MSGCOPTS) NetworkPacket.msg

PingMessage_m.o: PingMessage_m.cc
	$(CXX) -c $(NEDCOPTS) PingMessage_m.cc

PingMessage_m.cc PingMessage_m.h: PingMessage.msg
	$(MSGC) $(MSGCOPTS) PingMessage.msg


#doc: neddoc doxy

#neddoc:
#	opp_neddoc -a

#doxy: doxy.cfg
#	doxygen doxy.cfg

generateheaders: $(GENERATEDHEADERS)
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) generateheaders) || exit 1; done

clean:
	rm -f *.o *_n.cc *_n.h *_m.cc *_m.h .tstamp
	rm -f *.vec *.sca
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) clean); done

depend:
	$(MAKEDEPEND) $(INCLUDE_PATH) -- *.cc
	# $(MAKEDEPEND) $(INCLUDE_PATH) -fMakefile.in -- *.cc
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) depend) || exit 1; done

makefiles:
	# recreate Makefile
	opp_makemake -f  -n -c ../omnetppconfig -I../common 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefiles) || exit 1; done

makefile-ins:
	# recreate Makefile.in
	opp_makemake -f -m  -n -c ../omnetppconfig -I../common 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefile-ins) || exit 1; done

# "re-makemake" and "re-makemake-m" are deprecated, historic names of the above two targets
re-makemake: makefiles
re-makemake-m: makefile-ins



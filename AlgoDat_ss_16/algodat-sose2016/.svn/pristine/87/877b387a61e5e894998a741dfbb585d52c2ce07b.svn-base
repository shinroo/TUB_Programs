
/**
 * Represents a geometrical 3d Body
 * @author AlgoDat
 *
 */
public abstract class Body{
	
	/**
	 * Calculates the volume of a Body
	 * @return	The volume of a Body
	 */
	abstract double calculateVolume();
	
	/**
	 * Calculates the surface of a Body
	 * @return	The surface of a Body
	 */
	abstract double calculateSurface();
}

